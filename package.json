{
  "name": "scalar-set-theory",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --content-base dist/",
    "deploy": "sh ./deploy.sh",
    "test": "ava",
    "kill-test-server": "for /f \"tokens=5\" %a in ('netstat -aon ^| find \"8080\" ^| find \"LISTENING\"') do taskkill /f /pid %a"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.16.3",
    "immutable": "^3.8.1",
    "phantomjs-adapter": "0.0.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "latest",
      "react"
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
